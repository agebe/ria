#!/usr/bin/env ria

dependencies {
  'com.google.code.gson:gson:2.10'
}

var json = '''
[
	{
		color: "red",
		value: "#f00"
	},
	{
		color: "green",
		value: "#0f0"
	},
	{
		color: "blue",
		value: "#00f"
	},
	{
		color: "cyan",
		value: "#0ff"
	},
	{
		color: "magenta",
		value: "#f0f"
	},
	{
		color: "yellow",
		value: "#ff0"
	},
	{
		color: "black",
		value: "#000"
	}
]''';
var l = new Gson().fromJson(json, List.class);
println(l.get(0).get("value"));
